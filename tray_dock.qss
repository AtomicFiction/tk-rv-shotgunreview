QTreeView {
    border: 0px solid rgb(0,0,200);
}

QFrame {
    margin: 0px;
    padding: 0px;
    background: rgb(37,38,41);
}

QFrame#tray_frame {
    margin-top: 25px;
    padding-top: 0px;
    border: 0px solid rgb(200,100,50);
    min-height: 140px;
    min-width: 720px;
}

QFrame#tray_button_bar {
    margin: 0px;
    background: rgb(37,38,41);
    margin-bottom: 2px;
    margin-top: 0px;
    border: 0px solid rgb(200,100,100);
}

