QTabWidget::tab-bar {
    alignment: center;
}

QTabBar::tab:top {
    color: rgb(0, 0, 0);
    border: 1px solid black;
    border-top-left-radius: 5px;
    border-top-right-radius: 5px;
    padding-left: .4em;
    padding-right: .4em;
    padding-top: .1em;
    padding-bottom: .1em;
    min-width: 8ex;

}

QTabBar::tab:selected {
    border: 0px solid rgb(42,42,55);
    border-color: rgb(120,120,120);

}

QPushButton {
    color: rgb(200,200,200);
    border: 0px solid rgb(30,30,30);
    margin-left: 4px;
    margin-right: 4px;
    padding: 4px;
    border-radius: 0px;
}

QPushButton[latencyButton="true"] {
    font-size: 12pt;
}

QPushButton:disabled {
    color: rgb(90,90,90);
}

QPushButton:default:disabled {
    color: rgb(100,100,100);
}

QPushButton:pressed {
    background-color: rgb(0,0,0);
}

QPushButton:default {
    color: rgb(255,155,155);
    border: 1px solid rgb(30,30,55);
}
