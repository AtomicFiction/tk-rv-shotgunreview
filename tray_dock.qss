QListView {
    outline: none;
    icon-size: 0px;
    show-decoration-selected: 1;
    background-color: rgb(0, 0, 0);
    border: 0px solid rgb(0,0,0);
    padding: 0px;
}

QListView:selected {
    border:; 1px solid rgb(100,0,0);
    outline: none;
}

QListView#tray_list:selected {
    border: 0px solid rgb(200,0,0);
    outline: none;
}


QListView:item {
    margin: 0px;
    padding: 0px;
    outline: none;
}

#tray_list {
    background: rgb(37,38,41); 
    max-height: 150px;
    border: 0px solid rgb(0,0,0);
    margin: 0px;
    padding: 0px;
    outline: none;
    height: 130px;
}

QlistView:focus {
    outline: none;
}

QDockWidget::title {
    background: rgb(37,38,41);
}

QDockWidget {
    padding: 0px; 
    margin: 0px;
    color: rgb(200,200,200);
    border: 0px none;
}

QDockWidget::close-button {
    border: 1px solid rgb(30,30,30);
    float: left;
    /*border-radius: 6px;*/
}

QDockWidget::float-button {
    border: 1px solid rgb(30,30,30);
    float: left;
}

QDockWidget::title {
    background: rgb(36,38,41); 
    color: rgb(126,127,129); 
    text-align: center;

